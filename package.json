{
  "name": "react-transit",
  "version": "0.1.0",
  "description": "Components to display transit data in React map apps.",
  "license": "MIT",
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "babel-preset-react-app": "^9.0.0",
    "node-sass": "^4.11.0",
    "ol": "^5.3.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-datepicker": "^2.8.0",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-scripts": "3.0.1",
    "react-spatial": "^0.0.14-beta.62",
    "react-styleguidist": "^9.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf build/",
    "build": "buble --no modules,asyncAwait -i src -o build --objectAssign Object.assign --sourcemap, && cp -t build package.json README.md LICENSE && cp -rf src/images build",
    "postbuild": "cp package.json build/ && cd src && find .  -name '*.scss' | cpio -pdm ../build",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(jsts|ol|react-spatial|ole|react-datepicker)/).*/\"",
    "test:coverage": "yarn test --coverage --coverageDirectory=coverage",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "format": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "eject": "react-scripts eject",
    "doc": "bash ./scripts/prepare-doc.sh",
    "doc:server": "styleguidist server",
    "publish:public": "yarn version && git push origin HEAD && yarn build && yarn publish build/",
    "publish:beta": "yarn publish:public --tag beta",
    "cp": "generact --root src/components"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/runtime": "^7.3.1",
    "babel-plugin-named-asset-import": "^0.3.0",
    "canvas": "^2.3.1",
    "cypress": "^3.1.5",
    "documentation": "^12.1.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fetch-mock": "^7.2.5",
    "generact": "^0.4.0",
    "geops-docjs-template": "^1.0.0",
    "husky": "^2.4.1",
    "jest": "24.7.1",
    "jest-canvas-mock": "^2.0.0-alpha.2",
    "jest-serializer-html": "^7.0.0",
    "lint-staged": "^8.2.1",
    "node-fetch": "^2.3.0",
    "prettier": "^1.16.3",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
    "rewire": "^4.0.1",
    "stream-array": "^1.1.2",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.1",
    "vinyl-fs": "^3.0.3"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  }
}
